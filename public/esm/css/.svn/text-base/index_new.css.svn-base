/*动画类*/

.header_txt.active {
    opacity: 1;
    transition: opacity linear 1.5s;
}

.animat1.active {
    animation: yunmov1 1s 5 ease-in alternate;
}

.animat2.active {
    animation: yunmov2 0.8s 6 ease-in alternate;
}

.animat3.active {
    animation: yunmov3 1.2s 4 ease-in;
}

.animat4.active {
    animation: yunmov4 0.9s 5 ease-in alternate;
}

.animat5.active {
    animation: yunmov5 0.8s 6 ease-in alternate;
}

.animat6.active {
    animation: yunmov6 1.6s 4 ease-in alternate;
}

.animat7.active {
    animation: yunmov7 0.4s 10 ease-in alternate;
}

.animat8.active {
    animation: yunmov8 2s 3 ease-in alternate;
}

.animat9.active {
    animation: yunmov9 0.3s 15 ease-in alternate;
}

.animat10.active {
    animation: yunmov10 0.7s 6 ease-in alternate;
}

.animat11.active {
    animation: yunmov11 1.1s 5 ease-in alternate;
}

@keyframes yunmov1 {
    0% {
        transform: translate3d(0, 0, 0) rotate(10deg);
    }
    50% {
        transform: translate3d(-3px, -3px, 0) rotate(-20deg);
    }
}

@keyframes yunmov2 {
    0% {
        transform: translate3d(0, 0, 0);
    }
    50% {
        transform: translate3d(15px, 30px, 0);
    }
}

@keyframes yunmov3 {
    0% {
        transform: translate3d(0, 0, 0);
    }
    35% {
        transform: translate3d(-22px, -24px, -5px);
    }
    50% {
        transform: translate3d(-20px, -20px, 0);
    }
    65% {
        transform: translate3d(-22px, -24px, -5px);
    }
    100% {
        transform: translate3d(0, 0, 0);
    }
}

@keyframes yunmov4 {
    0% {
        transform: translate3d(0, 0, 0);
    }
    50% {
        transform: translate3d(20px, -60px, 0);
    }
}

@keyframes yunmov5 {
    0% {
        transform: translate3d(0, 0, 0);
    }
    50% {
        transform: translate3d(-20px, 20px, 0);
    }
}

@keyframes yunmov6 {
    0% {
        transform: translate3d(0, 0, 0);
    }
    50% {
        transform: translate3d(-5px, -5px, 0);
    }
}

@keyframes yunmov7 {
    0% {
        transform: translate3d(0, 0, 0);
    }
    50% {
        transform: translate3d(10px, 20px, 0);
    }
}

@keyframes yunmov8 {
    0% {
        transform: translate3d(0, 0, 0);
    }
    50% {
        transform: translate3d(-35px, 40px, 0);
    }
}

@keyframes yunmov9 {
    0% {
        transform: translate3d(0, 0, 0);
    }
    50% {
        transform: translate3d(5px, -20px, 0);
    }
}

@keyframes yunmov10 {
    0% {
        transform: translate3d(0, 0, 0);
    }
    50% {
        transform: translate3d(-30px, 5px, 0);
    }
}

@keyframes yunmov11 {
    0% {
        transform: translate3d(0, 0, 0);
    }
    50% {
        transform: translate3d(15px, 15px, 0);
    }
}

