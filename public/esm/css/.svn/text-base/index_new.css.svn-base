/*reset*/

body,
div,
p,
ul,
li,
dl,
dd,
dt,
a,
h1,
h2,
h3 {
    margin: 0;
    padding: 0;
    outline: none;
}

ul {
    list-style-type: none;
}

a {
    text-decoration: none;
}

img {
    border: 0;
}

body {
    min-width: 1000px;
    font: 12px/1.5 'HiraginoSans GB', 'Microsoft YaHei', 'WenQuanYi Micro Hei', Tahoma, Helvetica, Arial, "\5b8b\4f53", sans-serif;
}

