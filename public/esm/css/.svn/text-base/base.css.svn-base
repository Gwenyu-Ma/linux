/*base css*/

* {
    outline: none;
}

dd,
dt,
dl,
form,
ul,
ol,
li,
h3,
h4 {
    margin: 0;
    padding: 0;
}

ul {
    list-style-type: none;
}

body {
    font: 12px/1.5 'HiraginoSans GB', 'Microsoft YaHei', 'WenQuanYi Micro Hei', Tahoma, Helvetica, Arial, "\5b8b\4f53", sans-serif;
}

body,
html {
    height: 100%;
    min-height: 800px;
}

body {
    position: relative;
}

a {
    color: #47b8e3;
}

::-ms-clear {
    display: none;
}

::-ms-reveal {
    display: none;
}

.left {
    float: left;
}

.right {
    float: right;
}

.radio,
.checkbox {
    padding-left: 0px;
}

.control-group label.error {
    display: block;
    white-space: nowrap;
    position: absolute;
    bottom: -25px;
    left: 125px;
    *left: 135px;
    color: #f47838;
    font-size: 12px;
}

.control-group {
    position: relative;
}

.form-horizontal .control-label {
    width: 115px;
    padding-top: 6px;
    padding-right: 5px;
}

.form-horizontal .controls {
    padding-left: 120px;
    margin-left: 0;
    *padding-left: 10px;
}

.form-horizontal :first-child.controls,
.form-horizontal .ie-fixed-padding {
    padding-left: 120px;
    *padding-left: 130px;
}

.bg {
    position: fixed;
    _position: absolute;
    top: 0;
    left: 0;
    z-index: -10;
    min-width: 1000px;
    width: 100%;
    zoom: 1;
    height: 100%;
    background: url(../img/banner_3.jpg);
    background-repeat: no-repeat;
    background-position: center 0;
    background-size: cover;
}

.bg img {
    height: 100%;
    width: 100%;
    min-width: 1000px;
    position: fixed;
    top: 0;
    left: 0;
}

.bg2 {
    background: url(../img/banner_2.png) no-repeat;
}

.navbar {
    min-width: 960px;
}

.navbar-inner {
    background: url(../img/header_nav_bg.png) repeat;
    filter: none;
    border: 0;
    height: 70px;
}

.container-fluid {
    height: 100%;
}

.logo {
    line-height: 100%;
    padding-top: 10px;
    display: inline-block;
    /* width: 230px; */
}

.logo img {
    margin-right: 10px;
}

.logo_font {
    vertical-align: middle;
    color: #fff;
    font-size: 21px;
}

.content {
    width: 960px;
    height: 540px;
    margin: 40px auto 108px;
    background: #fff;
    box-shadow: 1px 1px 10px 5px rgba(0, 0, 0, 0.2);
}

.left_side {
    float: left;
    width: 418px;
    height: 100%;
    overflow: hidden;
}

.form-horizontal .control-group:after {
    clear: none;
}

.btn-purple,
.control-group.error .btn-purple {
    font-size: 18px;
    color: #fff;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
    background-color: #a481c8;
    background-image: -moz-linear-gradient(top, #9f7cc2, #8765a9);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#9f7cc2), to(#8765a9));
    background-image: -webkit-linear-gradient(top, #9f7cc2, #8765a9);
    background-image: -o-linear-gradient(top, #9f7cc2, #8765a9);
    background-image: linear-gradient(to bottom, #9f7cc2, #8765a9);
    background-repeat: repeat-x;
    border-color: #8765a9 #8765a9 #002a80;
    border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
    filter: progid: DXImageTransform.Microsoft.gradient(startColorstr='#9f7cc2', endColorstr='#8765a9', GradientType=0);
    filter: progid: DXImageTransform.Microsoft.gradient(enabled=false);
}

.btn-purple:hover,
.btn-purple:focus,
.btn-purple:active,
.btn-purple.active,
.btn-purple.disabled,
.btn-purple[disabled],
.control-group.error .btn-purple:hover {
    color: #fff;
    background-color: #8664a8;
}

.icon-info-sign {
    background: url(../img/ico08.png) no-repeat;
    width: 15px;
    height: 15px;
}

.checkbox {
    font-size: 12px;
}

.form-horizontal .control-group {
    margin-bottom: 28px;
}

.gray {
    color: #5e5e5e;
}

.footer {
    color: #d6e1ea;
    line-height: 26px;
    text-align: center;
    padding-top: 15px;
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 93px;
}

.bg2 .footer {
    color: #939393;
}

.slide {
    width: 100%;
    height: 100%;
}

.verify {
    display: inline-block;
    *display: inline;
    zoom: 1;
    border: 1px solid #d4d4d4;
    border-radius: 2px;
    height: 28px;
    vertical-align: top;
    width: 118px;
    overflow: hidden;
}

.verify img {
    float: left;
    width: 83px;
    height: 28px;
    border: 0;
}

.verify a {
    float: left;
    width: 34px;
    height: 30px;
    background: #f4f4f4 url(../img/ico10.png) no-repeat 0 0;
    text-decoration: none;
    border-radius: 2px;
    margin-top: -2px;
}

.verify a:hover {
    text-decoration: none;
    background-position: -40px 0;
}

.verify a:active {
    background-position: -80px 0;
}

.correct,
.wrong {
    background: url(../img/correct.png) no-repeat;
    padding-left: 60px;
    min-height: 50px;
    padding-bottom: 40px;
}

.wrong {
    background: url(../img/error.png) no-repeat;
}

.correct h3,
.wrong h3 {
    font-size: 18px;
    font-weight: normal;
    line-height: 24px;
    color: #5e5e5e;
    margin-bottom: 5px;
}

.correct h4,
.wrong h4 {
    font-size: 14px;
    font-weight: normal;
    line-height: 24px;
    color: #5e5e5e;
    margin-bottom: 5px;
}

.correct.w250,
.wrong.w250 {
    width: 190px;
}

input.btn.w250 {
    width: 250px;
}

.auto {
    margin-left: auto;
    margin-right: auto;
}

.ml10 {
    margin-left: 10px;
}

.mt10 {
    margin-top: 10px;
}

.ml60 {
    margin-left: 60px;
}

.ml7 {
    margin-left: 7px;
}

.ml120 {
    margin-left: 120px;
}

.ml200 {
    margin-left: 200px;
}

.pt10 {
    padding-top: 10px;
}

.pt40 {
    padding-top: 40px;
}

.mt50 {
    margin-top: 50px;
}

.mt30 {
    margin-top: 30px;
}

.mt20 {
    margin-top: 20px;
}

.mt8 {
    margin-top: 8px;
}

.mt4 {
    margin-top: 4px;
}

.mt25 {
    margin-top: 25px;
}

.orange {
    color: #f47838;
}

.blue {
    color: #3ec2f0;
}

.gray_ligth {
    color: #999;
}

.gray_deep {
    color: #b9b9b9;
}

.gray_ddep {
    color: #5e5e5e;
}

.w250 {
    width: 250px;
}

.w106 {
    width: 106px;
}

textarea:focus,
input[type="text"]:focus,
input[type="password"]:focus,
input[type="datetime"]:focus,
input[type="datetime-local"]:focus,
input[type="date"]:focus,
input[type="month"]:focus,
input[type="time"]:focus,
input[type="week"]:focus,
input[type="number"]:focus,
input[type="email"]:focus,
input[type="url"]:focus,
input[type="search"]:focus,
input[type="tel"]:focus,
input[type="color"]:focus,
.uneditable-input:focus {
    border-color: rgba(123, 89, 157, 0.8);
    outline: 0;
    outline: thin dotted \9;
    /* IE6-9 */
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(123, 89, 157, 0.6);
    -moz-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(123, 89, 157, 0.6);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(123, 89, 157, 0.6);
}

input.btn:focus,
a:hover,
a:focus {
    outline: 0;
}

.pwdStrong {
    display: inline-block;
    *display: inline;
    zoom: 1;
    padding-top: 0px;
    vertical-align: top;
    height: 20px;
    color: #c0c0c0;
}

.pwdStrong i {
    font-style: normal;
}

.pwdStrong li {
    float: left;
    margin-right: 5px;
    width: 28px;
    height: 2px;
    border: 1px solid #f47838;
}

.pwdStrong.low {}

.pwdStrong.low i {
    color: #f58145;
    margin-left: 5px;
}

.pwdStrong.low li {
    border: 1px solid #f47838;
}

.pwdStrong.low li.low {
    background-color: #f47838;
}

.pwdStrong.middle {}

.pwdStrong.middle i {
    color: #3ec2f0;
}

.pwdStrong.middle li {
    border: 1px solid #3ec2f0;
}

.pwdStrong.middle li.low,
.pwdStrong.middle li.middle {
    background-color: #3ec2f0;
}

.pwdStrong.high {}

.pwdStrong.high i {
    color: #15b300;
}

.pwdStrong.high li {
    border: 1px solid #15b300;
}

.pwdStrong.high li.low,
.pwdStrong.high li.middle,
.pwdStrong.high li.high {
    background-color: #15b300;
}

.hidden {
    display: none;
}

.visibili_hide {
    visibility: hidden;
}

.require {
    color: #f47838;
    vertical-align: middle;
    display: inline-block;
    *display: inlne;
    zoom: 1;
    padding-top: 3px;
    margin-left: 5px;
}

.f12 {
    font-size: 12px;
}

.f14 {
    font-size: 14px;
}

.checkR {
    display: inline-block;
    *display: inline;
    zoom: 1;
    background: url(../img/good.png) no-repeat 0 center;
    width: 19px;
    height: 24px;
}

::-webkit-input-placeholder {
    color: #d7d7d7!important;
}

::-moz-input-placeholder {
    color: #d7d7d7!important;
}

:-moz-input-placeholder {
    color: #d7d7d7!important;
}

:-ms-input-placeholder {
    color: #d7d7d7!important;
}

.msg-box {
    position: absolute;
    top: 30%;
    z-index: 9999;
    left: 50%;
    margin-left: -125px;
    min-width: 250px;
    *height: 0;
    box-shadow: none;
}

.msg-box .mask {
    position: absolute;
    top: -8px;
    left: -8px;
    right: -8px;
    bottom: -8px;
    background: #000;
    opacity: 0.15;
    filter: alpha(opacity=15);
    border-radius: 10px;
    z-index: -1;
}

.msg-box>div {
    background-color: #29bcef;
    color: #fff;
    margin: 0;
    border-radius: 8px;
    text-shadow: none;
    border: 0;
    padding: 15px 10px;
    text-align: center;
}

.msg-box>div>span {
    display: inline-block;
    line-height: 30px;
    background: url(../img/icon/alert_ico1.png) left center no-repeat;
    padding-left: 40px;
    font-size: 18px;
}

.msg-box>div.alert-error>span {
    background: url(../img/icon/alert_ico1.png) left center no-repeat;
}

.msg-box>div.alert-info>span {
    background: url(../img/icon/alert_ico3.png) left center no-repeat;
}

.msg-box>div.alert-success>span {
    background: url(../img/icon/alert_ico2.png) left center no-repeat;
}

.msg-box>div>h4 {
    *margin-top: -20px;
}

.alert .close {
    top: -12px;
    right: -2px;
}