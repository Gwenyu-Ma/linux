.client-list li a {
    position: relative;
    cursor: pointer;
    color: #000;
    height: 20px;
    padding: 5px 15px;
}

.client-list li .triangle-right {
    position: absolute;
    top: 14px;
    left: 20px;
    width: 0;
    height: 0;
    border-top: 5px solid transparent;
    border-left: 5px solid #9E9E9E;
    border-bottom: 5px solid transparent;
}

.client-list li.active .triangle-right {
    border-left: 5px solid #fff;
}

.client-list li.active .triangle-right.down {
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    border-top: 5px solid #fff;
    top: 17px;
    left: 16px;
}

.client-list .active>a,
.client-list .active>a:hover,
.client-list .active>a:focus {
    background-color: #666e9f;
    color: #fff;
}

.homeVersion .client-list .active>a,
