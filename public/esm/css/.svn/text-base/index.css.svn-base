    filter: alpha(opacity=0);
    opacity: 0;
    z-index: 1;
}

.upload_blg {
    display: inline-block;
    vertical-align: middle;
    margin-left: 30px;
}

.upload_btn,
.upload_btn:focus {
    display: block;
    width: 110px;
    height: 32px;
    line-height: 32px;
    font-size: 14px;
    color: #fff;
    font-family: '微软雅黑';
    background: url(../img/btn.png) no-repeat;
    text-align: center;
    text-decoration: none;
    margin: 0 auto;
    cursor: pointer;
}

.upload_btn:hover {
    color: #fff;
    text-decoration: none;
    background-position: 0 -40px;
}

.upload_btn:active {
    background-position: 0 -80px;
}

.upload_btn.process {
    background-position: 0 -120px;
}

.upload_btn i {
    width: 20px;
    height: 20px;
    line-height: 100%;
    display: inline-block;
    background: url(../img/loading.gif) no-repeat;
    vertical-align: middle;
}

