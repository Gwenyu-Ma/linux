/* message box */

.msg-box {
    position: absolute;
    top: 30%;
    z-index: 9999;
    left: 50%;
    margin-left: -125px;
    min-width: 250px;
    *height: 0;
    box-shadow: none;
    z-index: 99999;
}

.msg-box .mask {
    position: absolute;
    top: -8px;
    left: -8px;
    right: -8px;
    bottom: -8px;
    background: #000;
    opacity: 0.15;
    filter: alpha(opacity=15);
    border-radius: 10px;
    z-index: -1;
}

.msg-box>div {
    background-color: #29bcef;
    color: #fff;
    margin: 0;
    border-radius: 8px;
    text-shadow: none;
    border: 0;
    padding: 15px 10px;
    text-align: center;
}

.msg-box>div>span {
    display: inline-block;
    line-height: 30px;
    background: url(../img/icon/alert_ico1.png) left center no-repeat;
    padding-left: 40px;
    font-size: 18px;
}

.msg-box>div.alert-error>span {
    background: url(../img/icon/alert_ico1.png) left center no-repeat;
}

.msg-box>div.alert-info>span {
    background: url(../img/icon/alert_ico3.png) left center no-repeat;
}

.msg-box>div.alert-success>span {
    background: url(../img/icon/alert_ico2.png) left center no-repeat;
}

.msg-box>div>h4 {
    *margin-top: -20px;
}


