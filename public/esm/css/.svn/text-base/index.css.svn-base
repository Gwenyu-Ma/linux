html,
body {
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
    color: #444;
    overflow-x: auto;
    overflow-y: hidden;
    background: #f1f1f2;
}

html,
body,
input,
button,
select,
textarea {
    font-family: "Microsoft YaHei";
}

a:link,
a:visited,
a:active,
a:hover {
    *text-decoration: none;
    outline: none;
}

a:active,
input:active,
input:focus,
input {
    outline: none;
}

textarea {
    resize: none;
}

::-ms-clear {
    display: none;
}

::-ms-reveal {
    display: none;
}


